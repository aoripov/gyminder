<body>
{#{% form_theme form 'bootstrap_3_layout.html.twig' %}#}
<div id="fh5co-wrapper">
    <div id="fh5co-page">
        <div id="fh5co-header">
            <header id="fh5co-header-section">
                <div class="container">
                    {% for label, messages in app.flashes %}
                        {% for message in messages %}
                            <div class="alert alert-{{ label }} alert-dismissible fade show" role="alert">
                                {{ message }}
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                        {% endfor %}
                    {% endfor %}
                    <div class="nav-header">
                        <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
                        <h1 id="fh5co-logo"><a href="/">Gyminder</a></h1>
                        <!-- START #fh5co-menu-wrap -->
                        <nav id="fh5co-menu-wrap" role="navigation">
                            <ul class="sf-menu" id="fh5co-primary-menu">
                                <li class="active"><a href="/">Home</a></li>
                                {% if app.user %}
                                    <li><a href="/updateInfo">Update data</a></li>
                                    <li><a href="/addPair">Find partner</a></li>
                                    <li><a href="/addCluster">Add group</a></li>
                                    <li><a href="/addGym">Add gym location</a></li>
                                    <li><a class="btn btn-danger btn-xs" href="/logout">Logout</a></li>
                                {% else %}
                                    <li><a class="btn btn-primary btn-xs" href="/login">Login</a></li>
                                {% endif %}
                            </ul>
                        </nav>
                    </div>
                </div>
            </header>
        </div>
        {% block body %}{% endblock %}
    </div>
</div>

<footer>
    <div id="footer">
        <div class="container">
